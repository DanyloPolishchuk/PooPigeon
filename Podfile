# platform :ios, '9.0'

def shared_pods
   	use_frameworks!
	pod 'Google-Mobile-Ads-SDK'
	pod 'ReachabilitySwift'
    pod 'SecureDefaults', '1.0.5' # Swift 5.0
end

target 'PooPigeon - Debug' do
	# Pods for PooPigeon - Debug
	shared_pods
end

target 'PooPigeon - Release' do
	# Pods for PooPigeon - Release
	shared_pods
end

target 'PooPigeon - Release Paid' do
    # Pods for PooPigeon - Release Paid
    shared_pods
end
